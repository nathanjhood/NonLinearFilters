{
	"version": 2,
	"devCommand": "yarn dev",
	"installCommand": "yarn install",
	"buildCommand": "yarn build",
	"outputDirectory": ".",
	"public": false,
	"framework": null,
	"headers": [
		{
			"source": "/(.html)",
			"headers": [
				{
					"key": "X-UA-Compatible",
					"value": "ie-edge"
				},
				{
					"key": "Vercel-CDN-Cache-Control",
					"value": "max-age=3600"
				},
				{
					"key": "CDN-Cache-Control",
					"value": "max-age=60"
				},
				{
					"key": "Cache-Control",
					"value": "public, max-age=10, s-maxage=86400, immutable"
				},
				{
					"key": "X-XSS-Protection",
					"value": "1; mode=block"
				}
			]
		},
		{
			"source": "github-readme-stats.stoneydsp.com/(.*)",
			"headers": [
				{
					"key": "Vercel-CDN-Cache-Control",
					"value": "max-age=3600"
				},
				{
					"key": "CDN-Cache-Control",
					"value": "max-age=60"
				},
				{
					"key": "Cache-Control",
					"value": "public, max-age=10, s-maxage=86400, immutable"
				},
				{
					"key": "X-XSS-Protection",
					"value": "1; mode=block"
				}
			]
		},
		{
			"source": "skillicons.dev/",
			"headers": [
				{
					"key": "Vercel-CDN-Cache-Control",
					"value": "max-age=3600"
				},
				{
					"key": "CDN-Cache-Control",
					"value": "max-age=60"
				},
				{
					"key": "Cache-Control",
					"value": "public, max-age=10, s-maxage=86400, immutable"
				},
				{
					"key": "X-XSS-Protection",
					"value": "1; mode=block"
				}
			]
		},
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "X-Content-Type-Options",
					"value": "nosniff"
				},
				{
					"key": "X-Frame-Options",
					"value": "DENY"
				}
			]
		},
		{
			"source": "/public/(.*)",
			"headers": [
				{
					"key": "Access-Control-Allow-Origin",
					"value": "*"
				}
			]
		},
		{
			"source": "/:path*",
			"has": [
				{
					"type": "query",
					"key": "authorized"
				}
			],
		  "headers": [
				{
					"key": "x-authorized",
					"value": "true"
				}
			]
		},
		{
			"source": "/:path*",
			"missing": [
				{
					"type": "query",
					"key": "authorized"
				}
			],
		  "headers": [
				{
					"key": "x-authorized",
					"value": "false"
				}
			]
		}
	],
	"redirects": [
    {
      "source": "/.env",
			"destination": "/404.html"
		},
		{
			"source": "/.git/",
			"destination": "/404.html"
		},
		{
			"source": "/.git/config",
			"destination": "/404.html"
		},
		{
			"source": "/api/.git/config",
			"destination": "/404.html"
		},
		{
			"source": "/api/v1/.git/config",
			"destination": "/404.html"
		},
		{
			"source": "/api/v2/.git/config",
			"destination": "/404.html"
		},
		{
			"source": "/api/v3/.git/config",
			"destination": "/404.html"
		},
		{
			"source": "/api/v4/.git/config",
			"destination": "/404.html"
		},
		{
			"source": "/api/user/.git/config",
			"destination": "/404.html"
		},
		{
			"source": "/api/user/v1/.git/config",
			"destination": "/404.html"
		},
		{
			"source": "/api/user/v2/.git/config",
			"destination": "/404.html"
		},
		{
			"source": "/api/user/v3/.git/config",
			"destination": "/404.html"
		},
		{
			"source": "/api/user/v4/.git/config",
			"destination": "/404.html"
		},
		{
			"source": "/api/admin/.git/config",
			"destination": "/404.html"
		},
		{
			"source": "/api/admin/v1/.git/config",
			"destination": "/404.html"
		},
		{
			"source": "/api/admin/v2/.git/config",
			"destination": "/404.html"
		},
		{
			"source": "/api/admin/v3/.git/config",
			"destination": "/404.html"
		},
		{
			"source": "/api/admin/v4/.git/config",
			"destination": "/404.html"
		}
	]
}
